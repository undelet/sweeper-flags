{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier",
    "tslint-no-circular-imports"
  ],
  "defaultSeverity": "error",
  "jsRules": {},
  "rules": {
    "prettier": [true, { "singleQuote": true, "trailingComma": "all", "semi": false, "printWidth": 120 }],
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "no-console": false,
    "no-any": true,
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "comment-format": [false, "check-space", "check-uppercase"],
    "semicolon": [true, "never", "interfaces"],
    "no-shadowed-variable": false,
    "align": false,
    "ban": false,
    "class-name": true,
    "curly": true,
    "forin": true,
    "interface-name": [true, "never-prefix"],
    "jsdoc-format": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "label-position": true,
    "member-ordering": [true, "static-before-instance", "variables-before-functions"],
    "no-unused-variable": true,
    "no-arg": true,
    "no-switch-case-fall-through": true,
    "switch-default": true,
    "triple-equals": [true, "allow-null-check"],
    "typedef": [true, "parameter", "property-declaration"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "jsx-boolean-value": ["never"],
    "array-type": false,
    "no-bitwise": true,
    "no-construct": true,
    "no-debugger": true,
    "no-empty": [true, "allow-empty-catch"],
    "no-eval": true,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": [true, "check-parameters"],
    "no-floating-promises": true,
    "no-invalid-this": true,
    "no-string-throw": true,
    "no-unsafe-finally": true,
    "no-duplicate-imports": true,
    "no-empty-interface": true,
    "no-var-keyword": true,
    "deprecation": { "severity": "warning" },
    "prefer-for-of": true,
    "no-reference": true,
    "ordered-imports": true
  },
  "rulesDirectory": []
}
